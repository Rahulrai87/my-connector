async findAll() {
  return this.connectorModel.find().lean();
}

async findOne(id: string) {
  return this.connectorModel.findById(id).lean();
}

async remove(id: string) {
  await this.stateModel.deleteOne({ connectorId: id });
  await this.connectorModel.deleteOne({ _id: id });
  return { deleted: true };
}
