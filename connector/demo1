// src/connector/dto/create-connector.dto.ts
import { IsString, IsIn } from 'class-validator';

export class CreateConnectorDto {
  @IsString()
  name: string;

  @IsString()
  siteUrl: string;

  @IsString()
  cronExpression: string;

  // For now we validate strictly
  @IsIn(['sharepoint'])
  type: string;
}
